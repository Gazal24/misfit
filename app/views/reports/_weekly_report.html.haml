%table
  %tr
    %th
      Particulars
    - Branch.all.each do |branch|
      %th
        = branch.name
    %th
      Total
    - Marshal.load(@report.report).each do |r|
      %tr
        %th
          = r.keys[0]
        - Branch.all.each do |branch|
          %td
            = r[r.keys[0]][branch.id] || "-"
