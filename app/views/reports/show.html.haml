- if params[:report_type]=="PerformanceReport"
  = partial "#{params[:report_type].snake_case.gsub(" ","_")}"
- elsif @reports
  %h1
    = params[:report_type]
  - @reports.each do |report|
    = link_to "#{report.name}", url(:show_report, params[:report_type], report.id)
    %br
- else
  %h1
    =@report.name
  = partial "#{params[:report_type].snake_case.gsub(" ","_")}"
